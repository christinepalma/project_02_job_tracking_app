<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>



<%= form_for @user do |a| %>
First Name: <%= a.text_field :firstname %>
Last Name: <%= a.text_field :lastname %>
Username: <%= a.text_field :username %>
Email: <%= a.text_field :email %>
Password: <%= a.password_field :password %>
Password confirmation: <%= a.password_field :password_confirmation %>
<%= a.submit "add user"%>
<% end %>
